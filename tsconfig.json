{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": [
      "DOM",
      "ES2022"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ],
      "@/components/*": [
        "components/*"
      ]
    },
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}